<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="traingle" class="org.Package.SpringmoduleInitializingCollections.Traingle">
 <property name="points">
  <list>
   <ref bean="zeropoint"/>
   <ref bean="point2"/>
   <ref bean="point3"/>
  </list>
 </property>
</bean>

<bean id="zeropoint" class="org.Package.SpringmoduleInitializingCollections.Point">
 <property name="x" value="0"/>
 <property name="y" value="0"/>
</bean>

<bean id="point2" class="org.Package.SpringmoduleInitializingCollections.Point">
 <property name="x" value="20"/>
 <property name="y" value="0"/>
</bean>

<bean id="point3" class="org.Package.SpringmoduleInitializingCollections.Point">
 <property name="x" value="0"/>
 <property name="y" value="20"/>
</bean>

</beans>